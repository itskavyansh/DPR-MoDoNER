{
  "name": "@dpr-system/backend",
  "version": "1.0.0",
  "description": "Backend API server for DPR Quality Assessment System",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:simple": "tsx watch src/simple-server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:simple": "tsx src/simple-server.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.0",
    "cors": "^2.8.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.0",
    "mongodb": "^5.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.4.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "supertest": "^6.3.0",
    "tsx": "^3.12.0",
    "vitest": "^0.34.0"
  }
}
